{
  "manifestVersion": 1,
  "id": "gpt_pr_review",
  "name": "GPT Pull Request Review",
  "version": "0.0.1",
  "publisher": "piotrgredowski",
  "galleryFlags": ["Public", "Preview"],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "A task that review Pull Request using GPT model from OpenAI. Forked from Mustapha Larhrouch",
  "categories": ["Azure Pipelines"],
  "icons": {
    "default": "images/icon.png"
  },
  "tags": ["OpenAI", "GPT", "pullRequest", "review"],
  "files": [
    {
      "path": "gpt_pr_review"
    }
  ],
  "repository": {
    "type": "git",
    "uri": "https://github.com/piotrgredowski/azure-pipeline-gpt-pr-review"
  },
  "content": {
    "details": {
      "path": "overview.md"
    }
  },
  "contributions": [
    {
      "id": "gpt_pr_review",
      "type": "ms.vss-distributed-task.task",
      "targets": ["ms.vss-distributed-task.tasks"],
      "properties": {
        "name": "gpt_pr_review"
      }
    }
  ]
}
